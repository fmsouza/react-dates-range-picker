{
  "name": "react-dates-range-picker",
  "version": "v0.1.0",
  "description": "Small, stateless date / date range picker for react-dates.",
  "main": "lib/index.js",
  "jsnext:main": "src/index.js",
  "module": "src/index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src __test__",
    "compile": "babel -d lib src",
    "compile:watch": "babel -w -d lib src",
    "clean": "rm -rf lib",
    "prepublish": "npm run clean && npm run compile"
  },
  "files": [
    "*.md",
    "__test__",
    "LICENSE",
    "lib",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RafalFilipek/react-dates-range-picker.git"
  },
  "bugs": {
    "url": "https://github.com/RafalFilipek/react-dates-range-picker/issues"
  },
  "author": "Rafa≈Ç Filipek <rafal.filipek@gmail.com>",
  "license": "MIT",
  "keywords": [
    "date",
    "date-picker",
    "date-range-picker",
    "date-range",
    "react-dates"
  ],
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "./.coverage",
    "unmockedModulePathPatterns": [
      "node_modules/react/",
      "node_modules/enzyme/"
    ]
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-preset-latest": "6.24.1",
    "babel-preset-react": "6.24.1",
    "enzyme": "2.9.1",
    "eslint": "4.4.1",
    "eslint-config-airbnb": "15.1.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.2.1",
    "jest": "18.1.0",
    "publish-please": "2.3.1",
    "react-addons-shallow-compare": "^15.6.0",
    "react-addons-test-utils": "15.6.0",
    "react-dom": "15.6.1",
    "react-test-renderer": "15.6.1",
    "release": "1.4.0"
  },
  "dependencies": {
    "moment": "2.18.1",
    "react": "15.6.1",
    "react-dates": "12.4.0",
    "react-moment-proptypes": "1.5.0"
  }
}
